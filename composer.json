{
    "name": "zomars/bolt-store-hours",
    "description": "An extension to add business hours as a field type within Bolt",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "zomars",
            "email": "zomars@me.com"
        }
    ],
    "keywords": [
        "backend",
        "field",
        "storehours"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extensions\\Bolt\\StoreHours\\": "src",
            "StoreHours\\": "lib/StoreHours/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\StoreHours\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extensions\\Bolt\\StoreHours\\StoreHoursExtension",
        "bolt-screenshots": [
            "screenshot.png"
        ]
    }
}
